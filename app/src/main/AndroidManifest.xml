<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anyarscaner" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_gpt_cashback"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_gpt_cashback"
        android:supportsRtl="true"
        android:theme="@style/Theme.AnyarScaner"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MasukActivity">
        </activity>
        <activity android:name=".activity.ScannerActivity" />
        <activity android:name=".activity.CreateHisActivity" />
        <activity android:name=".activity.RiwayatActivity" />
        <activity android:name=".activity.ScanBarcodeActivity" />
        <activity android:name=".activity.EditProfilActivity" />
        <activity android:name=".activity.BantuanActivity" />
        <activity android:name=".activity.AboutActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA"/>

</manifest>